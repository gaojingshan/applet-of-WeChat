.topbox {
    height: 30px;
    padding: 6px 16px;
}

.topbox .kuang {
    width: 100%;
    height: 100%;
    background-color: #eee;
    border-radius: 200px;
    position: relative;
    padding: 5px 40px;
    box-sizing: border-box;
    font-size: 14px;
}

.topbox .kuang .fdj {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 5px;
    left: 4px;
}