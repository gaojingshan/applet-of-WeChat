<view>
	<scroll-view scroll-y="true" style="height:{{windowHeight - 51 }}px">
		<i-index height="100%" bind:change="onChange">
			<i-index-item wx:for="{{ allbs_arr }}" wx:key="index" wx:for-item="item" name="{{item.key}}" item-height="40">
				<view class="i-index-demo-item {{nowbrand == it ? 'cur' : ''}}" wx:for="{{item.list}}" wx:key="in" wx:for-item="it" bindtap="brand_click" data-brand="{{it}}">
					{{it}}
				</view>
			</i-index-item>
		</i-index>
	</scroll-view>
	<view class="bottom_btn">
		<view class="btn1">取消</view>
		<view class="btn2" bindtap="allbrand_okHan">确定</view>
	</view>
</view>